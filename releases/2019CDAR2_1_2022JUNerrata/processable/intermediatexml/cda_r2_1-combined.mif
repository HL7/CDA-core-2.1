          displayName="Rash"/&gt;
         &lt;statusCode code="completed"/&gt;
         &lt;targetSiteCode code="48856004" 
          codeSystem="2.16.840.1.113883.6.96" 
          codeSystemName="SNOMED CT" 
          displayName="Skin of palmer surface of index finger"&gt;
            &lt;qualifier&gt;
