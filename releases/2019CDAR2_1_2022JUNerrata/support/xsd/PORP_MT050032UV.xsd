         <xs:element name="author" type="PORP_MT050032UV.Author" nillable="true" minOccurs="0"
                     maxOccurs="unbounded">
            <xs:annotation>
               <xs:appinfo>
                  <mif:targetConnection name="author" minimumMultiplicity="0" maximumMultiplicity="*"
