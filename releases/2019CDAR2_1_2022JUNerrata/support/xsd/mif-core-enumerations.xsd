	<xs:simpleType name="ApprovalCategoryKind">
		<xs:annotation>
			<xs:documentation>The list of approval levels that can be associated with content in a ballot.</xs:documentation>
			<xs:documentation>Beta</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="votable">
				<xs:annotation>
					<xs:documentation>Indicates that the item is subject to approval</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="informative">
				<xs:annotation>
					<xs:documentation>Indicates that the item is not subject to approval but that feedback will be considered</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="non-votable">
				<xs:annotation>
					<xs:documentation>Indicates that the item is not subject to approval and that feedback will not be considered</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ApplicationRoleRelationshipKind">
		<xs:annotation>
			<xs:documentation>Possible relationships between application roles</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="AtLeastOne">
				<xs:annotation>
					<xs:documentation>The container application role must implement at least one, possibly more (including all) contained application roles.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Includes">
				<xs:annotation>
					<xs:documentation>Defines the relationship where the container contains the contents.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ArtifactGroupKind">
		<xs:annotation>
			<xs:documentation>A list of the types of groups of artifacts that can be referenced</xs:documentation>
			<xs:documentation>Alpha</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Footnotes">
				<xs:annotation>
					<xs:documentation>A reference to all of the footnotes defined within the artifact.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Packages">
				<xs:annotation>
					<xs:documentation>A reference to all of the packages defined within the artifact.  Only applies to packages.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Header">
				<xs:annotation>
					<xs:documentation>A reference to the header information associated with the artifact.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Annotations">
				<xs:annotation>
					<xs:documentation>A reference to all of the annotations defined within the artifact.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="History">
				<xs:annotation>
					<xs:documentation>A reference to all of the history associated with the artifact.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Derivation">
				<xs:annotation>
					<xs:documentation>A reference to all of the derivations associated with the artifact.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Properties">
				<xs:annotation>
					<xs:documentation>A reference to all of the properties within the artifact.  (Only applies to DatatypeDefinitions.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StaticModels">
				<xs:annotation>
					<xs:documentation>A reference to all of the static models defined within the artifact.  (Only applies to Packages.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SubjectAreas">
				<xs:annotation>
					<xs:documentation>A reference to all of the subject areas defined within the artifact.  (Only applies to static models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Classes">
				<xs:annotation>
					<xs:documentation>A reference to all of the classes defined within the artifact.  (Only applies to static models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StateEngines">
				<xs:annotation>
					<xs:documentation>A reference to all of the state engines defined within the artifact.  (Only applies to static models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="States">
				<xs:annotation>
					<xs:documentation>A reference to all of the states defined within the class.  (Only applies to classes within static models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StateTransitions">
				<xs:annotation>
					<xs:documentation>A reference to all of the state transitions defined within the class.  (Only applies to classes within static models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Associations">
				<xs:annotation>
					<xs:documentation>A reference to all of the associations defined within the class.  (Only applies to classes within static models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Attributes">
				<xs:annotation>
					<xs:documentation>A reference to all of the attributes defined within the class.  (Only applies to classes within static models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TriggerEvents">
				<xs:annotation>
					<xs:documentation>A reference to all of the trigger events defined within the artifact.  (Only applies to dynamic models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ApplicationRoles">
				<xs:annotation>
					<xs:documentation>A reference to all of the application roles defined within the artifact.  (Only applies to dynamic models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Interactions">
				<xs:annotation>
					<xs:documentation>A reference to all of the interactions defined within the artifact.  (Only applies to dynamic models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ReceiverResponsibilities">
				<xs:annotation>
					<xs:documentation>A reference to all of the xxx defined within the artifact.  (Only applies to interactions.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ConceptDomains">
				<xs:annotation>
					<xs:documentation>A reference to all of the vocabulary domains defined within the artifact.  (Only applies to vocabulary models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VocabularyCodes">
				<xs:annotation>
					<xs:documentation>A reference to all of the vocabulary codes defined within the artifact.  (Only applies to vocabulary models.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Templates">
				<xs:annotation>
					<xs:documentation>A reference to all of the templates defined within the artifact.  (Only applies to packages.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CommunicationProtocols">
				<xs:annotation>
					<xs:documentation>A reference to all of the communication protocols defined within the artifact.  (Only applies to packages.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ImplementationTechnologySpecifications">
				<xs:annotation>
					<xs:documentation>A reference to all of the implementation technology specifications defined within the artifact.  (Only applies to implementation technology specifications.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ArtifactKind">
		<xs:annotation>
			<xs:documentation>Identifies the kind of artifacts that can be packaged</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="IFC">
				<xs:annotation>
					<xs:documentation>Interface (CMET and STUB) definition</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RIM">
				<xs:annotation>
					<xs:documentation>Reference Information Model</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DIM">
				<xs:annotation>
					<xs:documentation>Domain Information Model (supercedes D-MIM)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SIM">
				<xs:annotation>
					<xs:documentation>Serializable Information Model (supercedes R-MIM, HMD and Message Type)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LIM">
				<xs:annotation>
					<xs:documentation>Localized Information Model (a.k.a. message profile)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DAM">
				<xs:annotation>
					<xs:documentation>Document Analysis Model</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DT">
				<xs:annotation>
					<xs:documentation>Datatype Model</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DC">
				<xs:annotation>
					<xs:documentation>Document</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GL">
				<xs:annotation>
					<xs:documentation>Glossary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SB">
				<xs:annotation>
					<xs:documentation>Story Board</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VO">
				<xs:annotation>
					<xs:documentation>Vocabulary Model</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AR">
				<xs:annotation>
					<xs:documentation>Application Role</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TE">
				<xs:annotation>
					<xs:documentation>Trigger Event</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IN">
				<xs:annotation>
					<xs:documentation>Interaction</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DMIM">
				<xs:annotation>
					<xs:documentation>Domain Message Information Model</xs:documentation>
					<xs:documentation>Deprecated: ?/?/? DMIM is replaced by DIM</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RM">
				<xs:annotation>
					<xs:documentation>Refined Message Information Model</xs:documentation>
					<xs:documentation>Deprecated: ?/?/? RMIM is replaced by SIM</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HD">
				<xs:annotation>
					<xs:documentation>Hierarchical Message Descriptor</xs:documentation>
					<xs:documentation>Deprecated: ?/?/? HMD is replaced by SIM</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MT">
				<xs:annotation>
					<xs:documentation>Message Type</xs:documentation>
					<xs:documentation>Deprecated: ?/?/? MT is replaced by SIM</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TS">
				<xs:annotation>
					<xs:documentation>Test Scenario</xs:documentation>
					<xs:documentation>Alpha</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EX">
				<xs:annotation>
					<xs:documentation>Instance Example</xs:documentation>
					<xs:documentation>Alpha</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ArtifactRenderingStyleKind">
		<xs:annotation>
			<xs:documentation>Defines the types of renderings that can occur for artifacts</xs:documentation>
			<xs:documentation>Alpha</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="GraphicalDetailed">
				<xs:annotation>
					<xs:documentation>A full-blown graphical rendition of the content</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GraphicalSummary">
				<xs:annotation>
					<xs:documentation>A high-level graphical rendition of the key elements within the content</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TableView">
				<xs:annotation>
					<xs:documentation>A table rendering of the content</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ExcelView">
				<xs:annotation>
					<xs:documentation>A specialized static-model rendering using an Excel-type view</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TextView">
				<xs:annotation>
					<xs:documentation>Marked-up text rendering of the content.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AttributePropertyKind">
		<xs:annotation>
			<xs:documentation>Possible types of attribute properties</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="isDocumentCharacteristic">
				<xs:annotation>
					<xs:documentation>If true, indicates that the specified attribute is a characteristic of the document (order, definition, criterion, etc.) rather than the described event.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="conductible">
				<xs:annotation>
					<xs:documentation>If true, indicates that the specified attribute can be conducted wen attribute conduction is enabled.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BindingIntensityKind">
		<xs:annotation>
			<xs:documentation>The degree to which conformance to the constraints of a Value Set Assertion are imposed upon a conformant application.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="MAY">
				<xs:annotation>
					<xs:documentation>The constraints expressed in the Value Set Assertion(s) should be considered a helpful suggestion but no conformance expectations are being asserted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHOULD">
				<xs:annotation>
					<xs:documentation>Applications are expected to obey the constraints expressed in the Value Set Assertion(s), but failure to do so will not be judged non-conformant.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHALL">
				<xs:annotation>
					<xs:documentation>Failure to observe the constraints expressed in the Value Set Assertion(s) will be judged non-conformant.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BusinessNameRoleKind">
		<xs:annotation>
			<xs:documentation>Indicates the purpose or role of the business name in a particular context. Intended primarily for use in the business name of mif:package and mif:freehandDocument, where it affords alternative and/or shorter renfitions of the title.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationString">
			<xs:enumeration value="secondaryTitle">
				<xs:annotation>
					<xs:documentation>In the context of a mif:package or mif:freeHandDocument provides an alternative title, such as might be required where a document has both an HL7 title and an ISO title.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="shortTitle">
				<xs:annotation>
					<xs:documentation>In the context of a mif:package or mif:freeHandDocument provides a shorter rendition of the "title" that can be used, for example" in page headers or footers, where full-length titles are inappropriate.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="shortSecondaryTitle">
				<xs:annotation>
					<xs:documentation>In the context of a mif:package or mif:freeHandDocument that has a secondaryTitle BusinessName, this provides a shorter rendition of the "secondaryTitle" that can be used, for example, in page headers or footers, where full-length titles are inappropriate.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ChangeRequestStatus">
		<xs:annotation>
			<xs:documentation>Provides the different 'types' of status for a change request</xs:documentation>
			<xs:documentation>Alpha</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Proposed">
				<xs:annotation>
					<xs:documentation>Change request has been submitted for consideration</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Decided">
				<xs:annotation>
					<xs:documentation>A decision has been made on the request (refer to 'resolution' to determine what the decision was . . .)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Implemented">
				<xs:annotation>
					<xs:documentation>The requested change (or agreed subset thereof) has been implemented</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ChangeResolutionKind">
		<xs:annotation>
			<xs:documentation>Provides the possible resolutions to a change request</xs:documentation>
			<xs:documentation>Alpha</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Approved">
				<xs:annotation>
					<xs:documentation>There is agreement to make the change (with any changes documented as part of the resolution) in the near term</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ApprovedDeferred">
				<xs:annotation>
					<xs:documentation>There is agreement to make the change (with any changes documented as part of the resolution), but timing of applicaiton of the change remains to be decided</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Rejected">
				<xs:annotation>
					<xs:documentation>The decision has been made to not proceed with the change.  Rationale for the refusal is documented in the resolution</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ClassPresentationKind">
		<xs:annotation>
			<xs:documentation>A list of the 'styles' of graphic images that can be associated with a class</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationString">
			<xs:enumeration value="HL7">
				<xs:annotation>
					<xs:documentation>Standard HL7 Visio representation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HL7Shadow">
				<xs:annotation>
					<xs:documentation>HL7 Visio shadow representation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UML">
				<xs:annotation>
					<xs:documentation>Traditional UML representation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ConceptDomainPropertyKind">
		<xs:annotation>
			<xs:documentation>A list of the types of properties a concept domain may have</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationString">
			<xs:enumeration value="ConceptualSpaceForClassCodeSystem">
				<xs:annotation>
					<xs:documentation>Specifies the code system for the classCode for which the domain defines the conceptual space</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ConceptualSpaceForClassCode">
				<xs:annotation>
					<xs:documentation>Specifies the classCode for which the domain defines the conceptual space</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ConceptRepresentationFormKind">
		<xs:annotation>
			<xs:documentation>A list of the forms a concept represenation might have</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="GIF">
				<xs:annotation>
					<xs:documentation>The representation is an icon stored as a uuencoded Graphic Interchange Format image.  (Be careful of intellectual property encumbrances on the format.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="JPEG">
				<xs:annotation>
					<xs:documentation>The representation is an icon stored as a uuencoded JPEG.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STRING">
				<xs:annotation>
					<xs:documentation>The representation is a string.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SVG">
				<xs:annotation>
					<xs:documentation>The representation is an icon stored as a uuencoded Scalable Vector Graphic image.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ConceptRepresentationUseKind">
		<xs:annotation>
			<xs:documentation>A list of the purposes a concept representation may be used for</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationString">
			<xs:enumeration value="ConceptId">
				<xs:annotation>
					<xs:documentation>Indicates that the representation may be used for internal and external references to the concept within terminology specifications.  For example when defining relationships between concepts.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Code">
				<xs:annotation>
					<xs:documentation>Indicates that the representation may be used when identifying the concept as part of a transmitted or persisted instance.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Display">
				<xs:annotation>
					<xs:documentation>Indicates that the representation may be used when exposing the concept to a human user.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SemanticDescriptor">
				<xs:annotation>
					<xs:documentation>Indicates that the representation may be used to uniquely express the semantics of the concept for human readability.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CMETEntryKind">
		<xs:annotation>
			<xs:documentation>Defines how the CMET may be entered</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Act">
				<xs:annotation>
					<xs:documentation>The base of the CMET or stub is a kind of Act, and may be entered via Participation or ActRelationship</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Entity">
				<xs:annotation>
					<xs:documentation>The base of the CMET or stub is a kind of Entity and may be entered via Role</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Role">
				<xs:annotation>
					<xs:documentation>The base of the CMET or stub is a Role.  May be entered via participation or along plays or scopes association from an Entity</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RolePlayedOrPerformed">
				<xs:annotation>
					<xs:documentation>The base of the CMET is a Role whose 'Scoping' association is already used or not intended to be used.  May be entered via participation or along plays association from an Entity</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RoleScopedOrPerformed">
				<xs:annotation>
					<xs:documentation>The base of the CMET is a Role whose 'Playing' association is already used or not intended to be used.  May be entered via participation or along scopes association from an Entity</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RolePerformedOnly">
				<xs:annotation>
					<xs:documentation>The base of the CMET is a Role whose 'Playing' and 'Scoping' associations are already used or not intended to be used.  May be entered via participation.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other">
				<xs:annotation>
					<xs:documentation>The base of the CMET or stub is one of the "Dark Blue" non-backbone classes.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodeFilterExpressionKind">
		<xs:annotation>
			<xs:documentation>Identifies a type of expression used to select a set of codes from a code-system</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="regex">
				<xs:annotation>
					<xs:documentation>The expression is a regular expression that must be true when applied to a code for it to be included.  Expression MUST be a valid regular expression, as used in w3c schema definition.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="tql">
				<xs:annotation>
					<xs:documentation>The expression is a Terminology Query Language (TQL) expression as defined in http://www.amia.org/pubs/symposia/D200562.PDF</xs:documentation>
					<xs:documentation>Alpha</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodingStrengthKind">
		<xs:annotation>
			<xs:documentation>Defines whether the specified element is restricted to only using the identified coding system for the element.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="CNE">
				<xs:annotation>
					<xs:documentation>If not null, the element must be coded and must be drawn from the value set specified in the binding.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CWE">
				<xs:annotation>
					<xs:documentation>If not null, the element must be coded if there is an appropriate value in the value set specified in the binding.  If no appropriate code is available, a local code may be used, or the value may be populated with only original text.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ConceptPropertyTypeKind">
		<xs:annotation>
			<xs:documentation>Indicates what types a CodedConcept property may have</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Boolean">
				<xs:annotation>
					<xs:documentation>Restricted to 'true' or 'false'</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Enumeration">
				<xs:annotation>
					<xs:documentation>Restricted to the values in the specified enumeration</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Integer">
				<xs:annotation>
					<xs:documentation>Restricted to integer values</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Real">
				<xs:annotation>
					<xs:documentation>Restricted to real (decimal) number values</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="String">
				<xs:annotation>
					<xs:documentation>Restricted to any character string</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Token">
				<xs:annotation>
					<xs:documentation>Restricted the contents allowed for xs:NMTOKEN</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ConceptRelationshipKind">
		<xs:annotation>
			<xs:documentation>Code representing a type of relationship between concepts or vocabulary domains.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Specializes">
				<xs:annotation>
					<xs:documentation>The child code is a more narrow version of concept represented by the parent code.  I.e. Every child concept is also a valid parent concept.  Used to allow determination of subsumption.  Must be transitive, irreflexive, antisymetric.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ComponentOf">
				<xs:annotation>
					<xs:documentation>The child code is a part of the concept represented by the parent code.  Used to determine allowed nesting, for example of address components.  Must be transitive, irreflexive, antisymetric.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GroupedBy">
				<xs:annotation>
					<xs:documentation>The child code is a part of a grouping represented by the parent code.  Used to determine navigational hierarchy not based on a specializes or component relationship.  E.g. ICD9 hierarchy.  Is transitive, irreflexive and antisymetric</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LessThan">
				<xs:annotation>
					<xs:documentation>For coded ordeinal code systems, indicates that the specified code is considered less than the related code.  Used to determine relationships for coded ordinals.  Must be transitive, irreflexive, antisymetric.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DefinitionallyQualifies">
				<xs:annotation>
					<xs:documentation>Identifies a code that can act as a qualifier for the referenced code, both as part of concept definition within the code system and as part of run-time concept definition to determine concepts allowed for use as qualifiers for a concept within a CD datatype.  Must be non-transitive, irreflexive.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NonDefinitionallyQualifies">
				<xs:annotation>
					<xs:documentation>Same as definitionally qualifies, but restricted to only being used at run-time.  Must be non-transitive, irreflexive.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Generalizes">
				<xs:annotation>
					<xs:documentation>Inverse of Specializes.  Only included as a derived relationship</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Component">
				<xs:annotation>
					<xs:documentation>Inverse of ComponentOf.  Only included as a derived relationship</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="InGroup">
				<xs:annotation>
					<xs:documentation>Inverse of GroupedBy.  Only included as a derived relationship</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GreaterThan">
				<xs:annotation>
					<xs:documentation>Inverse of LessThan.  Only included as a derived relationship</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DefinitionallyQualifiedBy">
				<xs:annotation>
					<xs:documentation>Inverse of DefinitionallyQualifies.  Only included as a derived relationship</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NonDefinitionallyQualifiedBy">
				<xs:annotation>
					<xs:documentation>Inverse of NonDefinitionallyQualifies.  Only included as a derived relationship</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other">
				<xs:annotation>
					<xs:documentation>The child code has a relationship with the parent that does not follow one of the pre-defined stereotypical patterns.  Any application behavior based on the relationship must be custom-coded.  Transitivity, Reflexivity and Symmetry must be asserted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ConceptUseKind">
		<xs:annotation>
			<xs:documentation>Indicates allowed values to identify how a concept can be used.</xs:documentation>
			<xs:documentation>Beta</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Analysis">
				<xs:annotation>
					<xs:documentation>Indicates that the concept is not "selectible" nor intended to be displayed to users when navigating the code system.  Its sole purpose is to support analysis by grouping like concepts for semantic reasons.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Communication">
				<xs:annotation>
					<xs:documentation>Indicates that the concept is "selectable"  It is used to communicate or persist information.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Navigation">
				<xs:annotation>
					<xs:documentation>Indicates that the concept is not "selectable", but is displayable to users to help them users navigate a hierarchy of concepts to find a concept that can be selected.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ConstructedElementKind">
		<xs:annotation>
			<xs:documentation>Indicates the type of element to construct.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="DatatypeSummary">
				<xs:annotation>
					<xs:documentation>A summary table of the primary datatypes.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ContentsLevelKind">
		<xs:annotation>
			<xs:documentation>Indicates how the element should appear in the table of contents</xs:documentation>
			<xs:documentation>Alpha</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="None">
				<xs:annotation>
					<xs:documentation>The element will not appear in the table of contents</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Detailed">
				<xs:annotation>
					<xs:documentation>The element will only appear in the 'detailed' table of contents</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Summary">
				<xs:annotation>
					<xs:documentation>The element will appear in both summary and detailed table of contents</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DatatypeKind">
		<xs:annotation>
			<xs:documentation>The list of allowed kinds of datatype definitions</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Definition">
				<xs:annotation>
					<xs:documentation>A universal datatype definition that defines new properties or semantics</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Binding">
				<xs:annotation>
					<xs:documentation>A universal datatype definition that asserts additional characterstics against at templated datatype when it has a particular binding</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Flavor">
				<xs:annotation>
					<xs:documentation>A constraint on a datatype definition</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DatatypeQualifierKind">
		<xs:annotation>
			<xs:documentation>The list of qualifiers for datatype references</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="datatype">
				<xs:annotation>
					<xs:documentation>Indicates that the referenced name is a datatype</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="parameter">
				<xs:annotation>
					<xs:documentation>Indicates that the referenced name is the name of one of the parent datatype's parameters</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="baseDiff">
				<xs:annotation>
					<xs:documentation>Indicates that the reference is to the "difference type" of the parent datatype</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="parameterDiff">
				<xs:annotation>
					<xs:documentation>Indicates that the reference is to the "difference type" of the parent's datatype parameter specified in "name"</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DatatypeOperationKind">
		<xs:annotation>
			<xs:documentation>The list of allowed datatype property kinds</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="promotion">
				<xs:annotation>
					<xs:documentation>The property converts a simpler datatype into a more complex datatype</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="demotion">
				<xs:annotation>
					<xs:documentation>The property exposes the datatype as a simpler datatype by defaulting properties of the new datatype and/or constraining the properties of the original datatype.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="literal">
				<xs:annotation>
					<xs:documentation>The property exposes the datatype as a string representation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="variableProperty">
				<xs:annotation>
					<xs:documentation>A property whose value may vary from instance to instance.  Variable properties will need to be comunicated or persisted by an ITS unless they can be inferred from the value of other properties.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DefaultDeterminerKind">
		<xs:annotation>
			<xs:documentation>Indicates places defaults can be drawn from</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="ITS">
				<xs:annotation>
					<xs:documentation>The value is determined by the Implementation Technology Specification used to encode the model referencing the datatype.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ReferencingAttribute">
				<xs:annotation>
					<xs:documentation>The value is determined by the definition of the attribute that references the datatype.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Realm">
				<xs:annotation>
					<xs:documentation>The value is determined by the realm in which the model instance is constructed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PropertyDefaultHandlingKind">
		<xs:annotation>
			<xs:documentation>Indicates code system properties are defaulted when not declared for a given concept.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Hierarchical">
				<xs:annotation>
					<xs:documentation>Non-specified properties are inherited from the nearest ancestor in the subsumption hierarchy.  If no ancestor has a declared value for the property, the concept property takes on the value of the property definition's default value, if any.  In circumstances of multiple inheritance where the different ancestors have a different property value, the property will be considered non-defined.  If the properity is mandatory, this would result in an error.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Flat">
				<xs:annotation>
					<xs:documentation>Non-specified properties are assumed to have the value declared in the property definition, if any.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DefinedMappingSourceKind">
		<xs:annotation>
			<xs:documentation>Lists 'pre-defined' standards that can be mapped to.  (Predefinition helps ensure consistency of spelling, capitalization, etc.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationString">
			<xs:enumeration value="HL7v2">
				<xs:annotation>
					<xs:documentation>mapping to the HL7 version 2.x messaging standard</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DICOM">
				<xs:annotation>
					<xs:documentation>mapping to the DICOM standard</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Analysis Model">
				<xs:annotation>
					<xs:documentation>mapping to domain analysis model</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DefinedRoleKind">
		<xs:annotation>
			<xs:documentation>Lists pre-defined roles that will commonly be used. (Ensures consistency of spelling, capitalization, etc.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationString">
			<xs:enumeration value="Committee co-chair">
				<xs:annotation>
					<xs:documentation>A person who was a co-chair of one of the contributing committees at the time of ballot.  (Identify the committee in brackets following the person's name)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Contact">
				<xs:annotation>
					<xs:documentation>A person to contact with questions about the ballot or its contents.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Editor">
				<xs:annotation>
					<xs:documentation>A person with responsibility for the consolidation and organization of the ballot material.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Primary Contributor">
				<xs:annotation>
					<xs:documentation>An individual who made substantial content contributions to the ballot material.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DerivationRelationshipKind">
		<xs:annotation>
			<xs:documentation>The allowed derivation relationships between two artifacts</xs:documentation>
			<xs:documentation>Beta</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="restriction">
				<xs:annotation>
					<xs:documentation>The current element is a proper subset of the element it was derived from, but is not identical.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="extension">
				<xs:annotation>
					<xs:documentation>The element derived from is a proper subset of this element, but is not identical.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="conflicting">
				<xs:annotation>
					<xs:documentation>Neither the element nor the element derived from are proper subsets of the other.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="annotated">
				<xs:annotation>
					<xs:documentation>The element is unchanged from the element it is derived from with the exception of descriptive annotations.  For datatype derivations, this effectively creates an Alias for an existing type.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unchanged">
				<xs:annotation>
					<xs:documentation>The element is unchanged from the element it is derived from.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DesignCommentTagKind">
		<xs:annotation>
			<xs:documentation>Flags allowed for design comments</xs:documentation>
			<xs:documentation>Alpha</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="TODO">
				<xs:annotation>
					<xs:documentation>Indicates additional actions which need to be taken.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FIXME">
				<xs:annotation>
					<xs:documentation>Indicates a situation where the existing content is known to be incomplete or incorrect.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NOTE">
				<xs:annotation>
					<xs:documentation>Indicates a generic comment about the design of the artifact.  E.g. Internal design rationale.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DocumentProfileKind">
		<xs:annotation>
			<xs:documentation>Indicates the scope of the document profile</xs:documentation>
			<xs:documentation>Alpha</xs:documentation>
		</xs:annotation>
		<xs:restriction base="EnumerationToken">
			<xs:enumeration value="Create">
				<xs:annotation>
					<xs:documentation>Profile relates to documents "as created".  Constraints reflect what sorts of documents can be constructed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Read">
				<xs:annotation>
					<xs:documentation>Profile relates to documents "as interpreted".  Constraints reflect what sorts of information the system can actually extract and use from a document.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Both">
				<xs:annotation>
					<xs:documentation>Profile relates to both how documents are created and read.  Constraints reflect the ability to both construct as well as extract and use data elements from the document</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DurationUnitsKind">
		<xs:annotation>
			<xs:documentation>Allowed units for measuring duration</xs:documentation>
			<xs:documentation>Alpha</xs:documentation>
		</xs:annotation>
