                     <mif:derivationSupplier staticModelDerivationId="3" className="Act" associationEndName="componentOf"/>
                     <mif:participantClass>
                        <mif:reference name="Component1"/>
                     </mif:participantClass>
                  </mif:targetConnection>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element name="subjectOf" type="PORP_MT050032UV.Subject4" nillable="true" minOccurs="0"
